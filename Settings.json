{
  "window.titleBarStyle": "native",
  "window.commandCenter": true,
  "security.workspace.trust.untrustedFiles": "open",
  "apc.activityBar": {
    "position": "bottom",
    "hideSettings": true,
    "size": 28
  },
  "apc.statusBar": {
    "position": "editor-bottom",
    "height": 28,
    "fontSize": 11
  },
  "apc.electron": {
    "titleBarStyle": "hidden",
    "trafficLightPosition": {
      // custom position
      "x": 10,
      "y": 10
    }
  },

  "workbench.colorCustomizations": {
    "statusBar.background": "#242529",
    "statusBar.noFolderBackground": "#242529",
    "statusBar.debuggingBackground": "#242529",
    "tab.activeBorder": "#3080f7",
    "editorGroupHeader.tabsBackground": "#242529",
    "tab.activeBackground": "#242529",
    "tab.inactiveBackground": "#242529",
    "activityBar.activeBorder": "#ffffff00",
    "statusBar.border": "#ffffff00",
    "activityBar.border": "#ffffff00",
    "scrollbar.shadow": "#ffffff00",
    "editorStickyScroll.border": "#ffffff24",
    "editorIndentGuide.activeBackground1": "#ffffff"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "workbench.iconTheme": "material-icon-theme",
  "update.mode": "none",
  "workbench.colorTheme": "MacOS Modern Dark - Xcode Default",
  "prettier.printWidth": 110,
  "window.customTitleBarVisibility": "never",
  "workbench.tree.enableStickyScroll": false
}
